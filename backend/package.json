{
  "name": "vc-provider",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "npx tsm ./src/main.ts",
    "test": "tsc -p tsconfig.json --noEmit && npm run test-other && npm run test-unit && npm run test-integraiton-no-db",
    "test-all": "tsc -p tsconfig.json --noEmit && npm run test-other && npm run test-unit && npm run test-integration",
    "test:watch": "./node_modules/.bin/watchlist src test --eager -- npm run test",
    "test-on-win": "set NODE_ENV=test && npx tsm ./node_modules/uvu/bin.js ./test",
    "test-other": "echo OTHER-TESTS && NODE_ENV=test npx tsm ./node_modules/uvu/bin.js ./test/other",
    "test-unit": "echo UNIT-TESTS && NODE_ENV=test npx tsm ./node_modules/uvu/bin.js ./test/unit",
    "test-integration": "echo INTEGRATION-TESTS-ALL && NODE_ENV=test npx tsm ./node_modules/uvu/bin.js ./test/integration",
    "test-integraiton-no-db": "echo INTEGRATION-TESTS-NO-DB && npm run test-integration-api && npm run test-integration-internal",
    "test-integration-api": "echo INTEGRATION-API-TESTs && NODE_ENV=test npx tsm ./node_modules/uvu/bin.js ./test/integration/API",
    "test-integration-internal": "echo INTEGRATION-INTERNAL-TESTS && NODE_ENV=test npx tsm ./node_modules/uvu/bin.js ./test/integration/internal",
    "test-integration-db": "echo INTEGRATION-DB-TESTS && NODE_ENV=test npx tsm ./node_modules/uvu/bin.js ./test/integration/DB"
  },
  "keywords": [],
  "type": "module",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.11.10",
    "@types/sinon": "^10.0.13",
    "prettier": "^2.8.3",
    "sinon": "^15.0.1",
    "tsm": "^2.3.0",
    "typescript": "^4.9.5",
    "uvu": "^0.5.6",
    "watchlist": "^0.3.1"
  },
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.2.0",
    "@fastify/static": "^6.9.0",
    "@fastify/swagger": "^8.3.0",
    "@fastify/swagger-ui": "^1.3.0",
    "@noble/ed25519": "^1.7.3",
    "@solana/web3.js": "^1.73.3",
    "@stablelib/sha256": "^1.0.1",
    "@sybil-center/sdk": "workspace:*",
    "ajv": "^8.12.0",
    "bitcoinjs-message": "^2.2.0",
    "bs58": "^5.0.0",
    "cache-clock": "^1.5.0",
    "deep-sort-object": "^1.0.2",
    "did-jwt": "^6.11.0",
    "dids": "^4.0.0",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "fastify": "^4.10.2",
    "fastify-cors": "^6.1.0",
    "fp-ts": "^2.13.1",
    "io-ts": "^2.2.20",
    "jsonwebtoken": "^9.0.0",
    "key-did-provider-ed25519": "^3.0.0",
    "key-did-resolver": "^3.0.0",
    "mongodb": "^5.6.0",
    "pino": "^8.10.0",
    "pino-pretty": "^9.3.0",
    "sort-keys": "^5.0.0",
    "twitter-api-v2": "^1.14.0",
    "typed-inject": "^3.0.1",
    "uint8arrays": "^4.0.3"
  },
  "files": [
    "./dist"
  ],
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pave1Pal/vc-provider.git"
  },
  "bugs": {
    "url": "https://github.com/Pave1Pal/vc-provider/issues"
  },
  "homepage": "https://github.com/Pave1Pal/vc-provider#readme",
  "description": ""
}
