{
  "name": "vc-provider",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "npx tsm ./src/main.ts",
    "test": "DB_URL=postgres://dev:dev@localhost:5434/dev pnpm run dev:test:up && NODE_ENV=test npx tsm ./node_modules/uvu/bin.js ./test && pnpm run dev:test:down",
    "test:watch": "./node_modules/.bin/watchlist src test --eager -- npm run test",
    "test-on-win": "set NODE_ENV=test && npx tsm ./node_modules/uvu/bin.js ./test",
    "dev:db:up": "docker compose -f dev/docker-compose.db-dev.yml up -d",
    "dev:db:down": "docker compose -f dev/docker-compose.db-dev.yml down",
    "dev:test:up": "pnpm run dev:db:down && pnpm run dev:db:up && pnpm run migration:push",
    "dev:test:down": "pnpm run dev:db:down",
    "migration:migrate": "drizzle-kit migrate",
    "migration:push": "drizzle-kit push --config=./drizzle.config.ts",
    "migration:pull": "drizzle-kit introspect --config=./drizzle.config.ts",
    "migration:generate": "drizzle-kit generate --config=./drizzle.config.ts"
  },
  "keywords": [],
  "type": "module",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.11.10",
    "@types/sinon": "^10.0.13",
    "prettier": "^2.8.3",
    "sinon": "^15.0.1",
    "tsm": "2.3.0",
    "typescript": "^4.9.5",
    "uvu": "0.5.6",
    "watchlist": "^0.3.1"
  },
  "dependencies": {
    "@fastify/cors": "^8.2.0",
    "@fastify/static": "^6.9.0",
    "@fastify/swagger": "^8.3.0",
    "@fastify/swagger-ui": "^1.3.0",
    "@keyvhq/core": "^2.1.1",
    "@keyvhq/postgres": "^2.1.1",
    "@noble/curves": "^1.2.0",
    "@noble/ed25519": "^1.7.3",
    "@solana/web3.js": "^1.73.3",
    "@stablelib/sha256": "^1.0.1",
    "@sybil-center/sdk": "workspace:*",
    "@zcredjs/core": "1.1.4",
    "@zcredjs/ethereum": "0.0.6",
    "@zcredjs/mina": "1.0.3",
    "bitcoinjs-message": "^2.2.0",
    "did-jwt": "^6.11.0",
    "dids": "^4.0.0",
    "dotenv": "^16.0.3",
