    "fastify": "4.23.2",
    "fastify-cors": "^6.1.0",
    "fastify-raw-body": "^4.2.2",
    "o1js": "0.16.2",
    "o1js-trgraph": "^2.2.0",
    "pino": "^8.10.0",
    "pino-pretty": "^9.3.0",
