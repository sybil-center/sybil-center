    "mina-signer": "3.0.0",
    "prettier": "^2.8.3",
    "sinon": "^15.0.1",
    "tsm": "^2.3.0",
    "typescript": "^4.9.5",
    "uvu": "^0.5.6",
    "watchlist": "^0.3.1"
  },
  "dependencies": {
    "@sybil-center/passport": "workspace:*",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^8.2.0",
    "@fastify/jwt": "^8.0.1",
    "@fastify/static": "^6.12.0",
    "@fastify/swagger": "^8.3.0",
    "@fastify/swagger-ui": "^1.3.0",
    "@jaljs/core": "^1.0.4",
    "@jaljs/js-zcred": "0.0.3",
    "@jaljs/o1js": "^1.1.5",
    "@keyvhq/core": "^2.1.1",
    "@keyvhq/postgres": "^2.1.1",
    "@zcredjs/core": "1.1.1",
    "dotenv": "^16.0.3",
